@model List<ResultTestimonialDto>

<!-- client section -->

<section class="client_section layout_padding-bottom mt-5">
    <div class="container">
        <div class="heading_container heading_center psudo_white_primary mb_45">
            <h2>
               Musterilerimiz Bizim Icin Ne Dediler
            </h2>
        </div>
        <div class="carousel-wrap row ">
            <div class="owl-carousel client_owl-carousel">
        @foreach (var item in Model)
        {
           
                    <div class="item">
                        <div class="box">
                            <div class="detail-box">
                                <p>
                                    @item.TestimonialComment
                                </p>
                                <h6>
                                    @item.TestimonialName
                                </h6>
                                <p>
                                    @item.TestimonialTitle
                                </p>
                            </div>
                            <div class="img-box">
                                <img src="@item.TestimonialImgUrl" alt="" class="box-img">
                            </div>
                        </div>
                    </div>

            
        }
            </div>
        </div>
    </div>
</section>

<!-- end client section -->